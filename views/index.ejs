<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>workout log create</title>
</head>
<body>
    <h1> <%= grindMSG %></h1>
  
    <table>
        <tr>
            <thead>Workout Name</thead>   
            <thead>Sets</thead>
            <thead>Reps</thead>
        </tr>
    <% workout.forEach(function(item){ %>  
        <tr>
            <td><%= item.name %></td>
            <td><%= item.sets %></td>
            <td><%= item.reps %></td>
        </tr>        
   <% }) %>
</table>
                
          
    <form action="/" method="post">
    
        <input type="text " name="newExr" placeholder="New Exercise" autocomplete="off"> 

        <!-- <button type="submit" name="button">+</button> -->

        <input type="text " name="setNum" placeholder="set #" autocomplete="off"> 

        <input type="text " name="repsNum" placeholder="set #" autocomplete="off"> 

        <button type="submit" name="button">+</button>

    
    </form>

    <!-- <form action="/" method="post">
    
        <input type="text " name="setNum" placeholder="set #" autocomplete="off"> 

        <button type="submit" name="button">++</button>
    </form> -->
</body>
</html>