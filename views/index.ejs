<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>workout log create</title>
</head>
<body>
    <h1> <%= grindMSG %></h1>
  
    
    <% workout.forEach(function(item){ %>
        <h2> <%= item.name %></h2>
        <h2> <%= item.sets %></h2>
        <h2> <%= item.reps %></h2>
   <% }) %>
                
          
    <form action="/" method="post">
    
        <input type="text " name="newExr" placeholder="New Exercise" autocomplete="off"> 

        <!-- <button type="submit" name="button">+</button> -->

        <input type="text " name="setNum" placeholder="set #" autocomplete="off"> 

        <input type="text " name="repsNum" placeholder="set #" autocomplete="off"> 

        <button type="submit" name="button">+</button>

    
    </form>

    <!-- <form action="/" method="post">
    
        <input type="text " name="setNum" placeholder="set #" autocomplete="off"> 

        <button type="submit" name="button">++</button>
    </form> -->
</body>
</html>